<?xml version="1.0"?>
<config version="8.1.0" urldb="paloaltonetworks">
  <mgt-config>
    <devices/>
    <users>
      <entry name="admin">
        <phash>*</phash>
        <permissions>
          <role-based>
            <superuser>yes</superuser>
          </role-based>
        </permissions>
      </entry>
      <entry name="pandemo">
        <public-key></public-key>
        <permissions>
          <role-based>
            <superuser>yes</superuser>
          </role-based>
        </permissions>
        <phash>$1$knjualxm$iYQpdU.HxZy9CoN7y3RZh.</phash>
      </entry>
    </users>
  </mgt-config>
  <shared/>
  <devices>
    <entry name="localhost.localdomain">
      <deviceconfig>
        <system>
          <update-server>updates.paloaltonetworks.com</update-server>
          <update-schedule>
            <threats>
              <recurring>
                <weekly>
                  <day-of-week>wednesday</day-of-week>
                  <at>01:02</at>
                  <action>download-only</action>
                </weekly>
              </recurring>
            </threats>
          </update-schedule>
          <timezone>US/Pacific</timezone>
          <service>
            <disable-telnet>yes</disable-telnet>
            <disable-http>yes</disable-http>
          </service>
          <type>
            <dhcp-client>
              <send-hostname>yes</send-hostname>
              <send-client-id>no</send-client-id>
              <accept-dhcp-hostname>no</accept-dhcp-hostname>
              <accept-dhcp-domain>no</accept-dhcp-domain>
            </dhcp-client>
          </type>
          <hostname>jppanormaAWS21</hostname>
        </system>
        <setting>
          <management>
            <storage-partition>
              <internal/>
            </storage-partition>
            <hostname-type-in-syslog>FQDN</hostname-type-in-syslog>
            <initcfg>
              <type>
                <dhcp-client>
                  <send-hostname>yes</send-hostname>
                  <send-client-id>no</send-client-id>
                  <accept-dhcp-hostname>no</accept-dhcp-hostname>
                  <accept-dhcp-domain>no</accept-dhcp-domain>
                </dhcp-client>
              </type>
              <hostname>jppanormaAWS21</hostname>
              <username>pandemo</username>
              <public-key></public-key>
            </initcfg>
          </management>
        </setting>
      </deviceconfig>
      <template>
        <entry name="AWS-spoke1">
          <settings>
            <default-vsys>vsys1</default-vsys>
          </settings>
          <config>
            <devices>
              <entry name="localhost.localdomain">
                <vsys>
                  <entry name="vsys1">
                    <zone>
                      <entry name="untrust">
                        <network>
                          <layer3>
                            <member>ethernet1/1</member>
                          </layer3>
                        </network>
                      </entry>
                      <entry name="trust">
                        <network>
                          <layer3>
                            <member>ethernet1/2</member>
                          </layer3>
                        </network>
                      </entry>
                    </zone>
                    <import>
                      <network>
                        <interface>
                          <member>ethernet1/1</member>
                          <member>ethernet1/2</member>
                        </interface>
                      </network>
                    </import>
                  </entry>
                </vsys>
                <deviceconfig>
                  <system>
                    <fqdn-refresh-time>60</fqdn-refresh-time>
                    <dns-setting>
                      <servers>
                        <primary>169.254.169.253</primary>
                      </servers>
                    </dns-setting>
                  </system>
                  <setting>
                    <aws-cloudwatch>
                      <enabled>yes</enabled>
                      <name>VMseries-AWS</name>
                    </aws-cloudwatch>
                  </setting>
                </deviceconfig>
                <network>
                  <virtual-router>
                    <entry name="VR-AWS-tmplspoke1">
                      <ecmp>
                        <algorithm>
                          <ip-modulo/>
                        </algorithm>
                      </ecmp>
                      <protocol>
                        <bgp>
                          <routing-options>
                            <graceful-restart>
                              <enable>yes</enable>
                            </graceful-restart>
                          </routing-options>
                          <enable>no</enable>
                        </bgp>
                        <rip>
                          <enable>no</enable>
                        </rip>
                        <ospf>
                          <enable>no</enable>
                        </ospf>
                        <ospfv3>
                          <enable>no</enable>
                        </ospfv3>
                      </protocol>
                      <interface>
                        <member>ethernet1/1</member>
                        <member>ethernet1/2</member>
                      </interface>
                    </entry>
                  </virtual-router>
                  <interface>
                    <ethernet>
                      <entry name="ethernet1/1">
                        <layer3>
                          <ipv6>
                            <neighbor-discovery>
                              <router-advertisement>
                                <enable>no</enable>
                              </router-advertisement>
                            </neighbor-discovery>
                          </ipv6>
                          <ndp-proxy>
                            <enabled>no</enabled>
                          </ndp-proxy>
                          <lldp>
                            <enable>no</enable>
                          </lldp>
                          <dhcp-client/>
                        </layer3>
                      </entry>
                      <entry name="ethernet1/2">
                        <layer3>
                          <ipv6>
                            <neighbor-discovery>
                              <router-advertisement>
                                <enable>no</enable>
                              </router-advertisement>
                            </neighbor-discovery>
                          </ipv6>
                          <ndp-proxy>
                            <enabled>no</enabled>
                          </ndp-proxy>
                          <dhcp-client>
                            <create-default-route>no</create-default-route>
                          </dhcp-client>
                          <lldp>
                            <enable>no</enable>
                          </lldp>
                        </layer3>
                      </entry>
                    </ethernet>
                  </interface>
                </network>
              </entry>
            </devices>
            <mgt-config>
              <users>
                <entry name="pandemo">
                  <permissions>
                    <role-based>
                      <superuser>yes</superuser>
                    </role-based>
                  </permissions>
                  <phash>$1$evenskdt$yXk24AYuRqJJhK5S9BSWI/</phash>
                </entry>
              </users>
            </mgt-config>
          </config>
        </entry>
      </template>
      <device-group>
        <entry name="AWS-dgspoke1">
          <devices/>
          <pre-rulebase>
            <security>
              <rules>
                <entry name="Allow_HTTP">
                  <target>
                    <negate>no</negate>
                  </target>
                  <to>
                    <member>trust</member>
                  </to>
                  <from>
                    <member>untrust</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>web-browsing</member>
                  </application>
                  <service>
                    <member>any</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                  <log-start>yes</log-start>
                  <log-end>no</log-end>
                </entry>
                <entry name="Outbound_All">
                  <target>
                    <negate>no</negate>
                  </target>
                  <to>
                    <member>untrust</member>
                  </to>
                  <from>
                    <member>trust</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>any</member>
                  </application>
                  <service>
                    <member>any</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                  <log-start>yes</log-start>
                  <log-end>no</log-end>
                </entry>
              </rules>
            </security>
          </pre-rulebase>
        </entry>
      </device-group>
      <template-stack>
        <entry name="AWS-tmplspoke1">
          <templates>
            <member>AWS-spoke1</member>
          </templates>
        </entry>
      </template-stack>
    </entry>
  </devices>
  <readonly>
    <max-internal-id>15</max-internal-id>
    <devices>
      <entry name="localhost.localdomain">
        <template>
          <entry name="AWS-spoke1">
            <id>11</id>
            <config>
              <devices>
                <entry name="localhost.localdomain">
                  <vsys>
                    <entry name="vsys1">
                      <zone>
                        <entry name="untrust">
                          <id>13</id>
                        </entry>
                        <entry name="trust">
                          <id>14</id>
                        </entry>
                      </zone>
                    </entry>
                  </vsys>
                </entry>
              </devices>
            </config>
          </entry>
        </template>
        <device-group>
          <entry name="AWS-dgspoke1">
            <id>12</id>
          </entry>
        </device-group>
        <template-stack>
          <entry name="AWS-tmplspoke1">
            <id>15</id>
          </entry>
        </template-stack>
      </entry>
    </devices>
  </readonly>
</config>
